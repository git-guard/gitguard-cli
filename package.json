    "registry": "https://registry.npmjs.org/"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "dependencies": {
    "axios": "^1.7.7",
    "commander": "^14.0.2",
    "ignore": "^7.0.5",
    "open": "^8.4.2"
  },
  "devDependencies": {
    "@types/jest": "^29.5.14",
    "@types/node": "^22.0.0",
    "jest": "^29.7.0",
    "ts-jest": "^29.2.5",
    "tsx": "^4.20.0",
    "typescript": "^5.6.0"
  },
  "files": [
    "dist",
    "LICENSE",
    "README.md"
  ]
