{
  "name": "@gitguard/cli",
  "version": "1.1.2",
  "description": "GitGuard CLI - Security scanning for developers",
  "main": "dist/index.js",
  "bin": {
    "gitguard": "./dist/index.js"
  },
  "scripts": {
    "build": "tsc",
    "dev": "tsx src/index.ts",
    "prepublishOnly": "yarn build",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage"
  },
  "keywords": [
    "security",
    "scanner",
    "cli",
    "vulnerability",
    "code-analysis",
    "static-analysis"
  ],
  "author": "GitGuard",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/git-guard/gitguard-cli.git"
  },
  "bugs": {
    "url": "https://github.com/git-guard/gitguard-cli/issues"
  },
  "homepage": "https://gitguard.net",
  "publishConfig": {
